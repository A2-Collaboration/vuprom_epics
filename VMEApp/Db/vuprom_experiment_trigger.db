record(ai, "DAQ:Livetime_vme-tagg-tdc-a")
{
    field(DTYP, "VMEScaler")
        field(INP, "0xEA003000:94")
        field(SCAN, "I/O Intr")
        field(DESC, "Livetime of CPU vme-tagg-tdc-a")
		field(EGU, "%")
		field(HOPR, 100)
		field(LOPR, 0)
		field(PREC, 1)
		field(LINR, "SLOPE")
		field(ESLO, 0.0001)
		field(HIGH, 101)
		field(LOW, 70)
		field(HIHI,101)
		field(LOLO, 50)
		field(LSV, "MINOR")
}

