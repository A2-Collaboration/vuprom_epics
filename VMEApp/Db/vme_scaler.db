#  Record database for TAPS Trigger VUPROM Scalser
#			O.Steffen <steffen@kph.uni-mainz.de>, Sep 27, 2013
#
#  Address format:  0xAA0BBCCC
#
#  AA: Module address. example 0A
#  BB: Subaddress. example 03
#  CCC: Scaler value address
#

record(ai, "DAQ:TAPSBusy")
{
    field(DTYP, "VMEScaler")
        field(INP, "Addr: 0x0A003000")
        field(SCAN, "I/O Intr")
        field(DESC, "TAPSBusy")
}

