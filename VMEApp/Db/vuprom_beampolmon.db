#  Record database for vme-beampolmon
#
#	O.Steffen <steffen@kph.uni-mainz.de>, Oct 23, 2013
#
#  Address format:  0xAA0BB000:XXX
#
#  AA: Module address. example 0A (hex)
#  BB: Subaddress. example 03 (hex)
#  XXX: Scaler index (decimal)
#

### vuprom 0x01

record(ai, "DAQ:Tagger_16_OR_I")
{
	field(DTYP, "VMEScaler")
	field(INP, "0x01014000:0")
	field(SCAN, "I/O Intr")
	field(DESC, "Tagger 16-OR Rate of Section I")
	field(LOPR, 0)
	field(HOPR, 0)
	field(EGU,"Hz")
}

