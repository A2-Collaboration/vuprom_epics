#  Record database for TAPS Trigger VUPROM Scalser
#			O.Steffen <steffen@kph.uni-mainz.de>, Sep 27, 2013
#
#  Address format:  0xAA0BB000:XXX
#
#  AA: Module address. example 0A (hex)
#  BB: Subaddress. example 03 (hex)
#  XXX: Scaler index (decimal)
#
record(ai, "DAQ:TAPSBusy")
{
	field(DTYP, "VMEScaler")
	field(INP, "0x0A003000:0")
	field(SCAN, "I/O Intr")
	field(DESC, "TAPSBusy")
	field(LOPR, 0)
	field(HOPR, 0)
}

